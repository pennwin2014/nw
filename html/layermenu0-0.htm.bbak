<html>
<head>
<title></title>
<link REL=stylesheet HREF="/css/runmit.css" TYPE='text/css'>
</head>
<script>
function mouseovertr2 (o){
var i;
df=o.style.backgroundColor
	o.style.backgroundColor= "#000066";
	o.style.borderColor= "#cccccc";
	o.style.cursor="hand";	
for(i=0; i<o.cells.length; i++)
{
	o.cells(i).style.color="#ffffff";
}	
}

function mouseouttr2(o){
	o.style.backgroundColor=df
for(i=0; i<o.cells.length; i++)
{
	o.cells(i).style.color="#000000";
}
	
	
}
function menu_frame_onmouseover()
{
	parent.clearTimeout(parent.popTimer);
	//alert('parent cleartimeout ok!');
	document.body.className='highlight_container'}
function menu_frame_onmouseout()
{
	document.body.className="menu_container";
	parent.popOut()}
//document.onmouseover=menu_frame_onmouseover;
//document.onmouseout=menu_frame_onmouseout;
function goto(t){
   parent.crm_right.location.href=t
}

</script>
<body topMargin=0 leftmargin=0 marginheight="0" class=menu_space style='cursor:default;border:0' fixed oncontextmenu="return false">
<table class=3dtable_notop1 border=0 width=100% cellspacing=0 cellpadding=0 style='padding:2px' height="68">
  <tr onmouseover="mouseovertr2(this)" onmouseout="mouseouttr2(this)" onclick="goto('/pronline/Msg?FunName@crmProdInfoEdit')"> 
    <td height="22">&nbsp;<img src="/images/blank.gif" width="18" height="18" align="absmiddle">产品信息维护</td>
  </tr>
  <tr onmouseover="mouseovertr2(this)" onmouseout="mouseouttr2(this)" onclick="goto('/pronline/Msg?FunName@crmUserInfoEdit')"> 
    <td height="22">&nbsp;<img src="/images/blank.gif" width="18" height="18" align="absmiddle">客户资料维护</td>
  </tr>
  <tr id=main > 
    <td height="2"><img src="/images/hr.gif" width="100%" height="2"></td>
  </tr>
  <tr onmouseover="mouseovertr2(this)" onmouseout="mouseouttr2(this)" > 
    <td height="22">&nbsp;<img src="/images/blank.gif" width="18" height="18" align="absmiddle">退出</td>
  </tr>
</table>
</body>
</html>