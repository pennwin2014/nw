<body marginheight=0 leftmargin=0 marginwidth=0 topMargin="0" BGCOLOR="#DFF0F7" style="overflow:hidden" >
<form action="/pronline/upload" method="post" enctype="multipart/form-data" name="form1" >  

		<input type="hidden" name="add" value="">
		<input type="hidden" name="modi" value="">
	<input type="hidden" name="FunName" value="ncWebSoft_upload">
	<input type="hidden" name="UploadPath" value="/home/ncmysql/nc/upload">
  <input type="hidden" name="num" value="1">
  <input type="hidden" name="start" value="0">
  <Input type="hidden" name="commd" value="">
  <input type="hidden" name="refresh_flag" value="1">
  <input type="hidden" name="command" value="">
	<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0" >
    <tr >
      <td valign="top" class="gdt-bg-04" height="22" >
        <table border="0" width="100%" cellspacing="0" cellpadding="0">
          <tr class="title">
         
            <td >工具&gt;&gt;软件升级</td>
            <td >&nbsp;</td>
          </tr>
        </table>
      </td>
    </tr>
   
  </table>
	
	
  <table border="0" width=100%  cellspacing="0" cellpadding="0" align=center>
    <tr> 
      <td width="100%" valign="top" class="ttt"> 
        <table width="100%" border="0" cellpadding="0" cellspacing="0">
         
          <tr bgcolor="#DFF0F7"> 
            <td width="28%" height="34">&nbsp;上传软件包
           <input type="file" name="fname" style="width:400px">

            <input name="cmd"  id="cmd1" type="submit" class="an3" value="上传" Onclick="if(form1.fname.value=='') {alert('请先上传软件包');return false;};document.all.ff.innerHTML='<br>&nbsp;<font size=3><strong>正在上传，请稍等...</strong></font>';form1.start.value='1';form1.FunName.value='ncWebSoft_upload';form1.refresh_flag.value='1';">
            <input name="upd" id="cmd2" type="submit" class="an3" value="升级" Onclick="form1.reset();document.all.ff.innerHTML='<br>&nbsp;<font size=3><strong>正在升级，请稍等...</strong></font>';form1.FunName.value='ncWebSoft_sjml';form1.submit();">
            <input name="syn" id="cmd3" type="submit" class="an3-1" value="同步数据结构" Onclick="form1.reset();document.all.ff.innerHTML='<br>&nbsp;<font size=3><strong>正在同步数据结构，请稍等...</strong></font>';form1.FunName.value='ncWebSoft_sjml';form1.submit();">
            <input name="cl" type="button" class="an3" value="清屏" Onclick="document.all.ff.innerHTML='';">
             <input name="cl" type="button" class="an3" value="重启系统" Onclick="shutdown('11')">
            （软件包必须为zip格式）
                </td>
                
             </td>
          </tr>
        </table>
      </td>
