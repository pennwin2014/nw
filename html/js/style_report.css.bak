/*检查是否输入非法字符*/
.input2 {
 background-color: #cccccc;
 border: 1px #666666 solid;
 font-size: 12px;
 clip:rect( );
onkeydown: expression(onkeydown=function () {
if(event.keyCode==13) {
event.keyCode = 0;
event.returnvalue = false;
}
}
);
onblur: expression(onblur=function () {
if(this.value.indexOf("<")!=-1||this.value.indexOf(">")!=-1) {
alert("不能包含字符'<','>'");
focus();
}
}
);
}
