    groupid   int unsigned default 0,                  -- 部门Id
    devname   char(32)  default ' ',                       -- 设备名
    udisp     char(32)  default '',                        -- 用户名
    sip       int unsigned,                                -- sip
    procname  char(72),                                    -- 进程名称
    mark      char(1),                                                             -- 操作类型 插入，拔出
    stime     int unsigned,                                -- 操作时间
