<html>
<head>
<title>Groupinfo</title>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312">
<META HTTP-EQUIV="Expires" CONTENT="0">
<META HTTP-EQUIV="Cache-Control" CONTENT="no-cache, must-revalidate">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
<META HTTP-EQUIV="Jon Eyrick" CONTENT="no-cache">

<link rel="stylesheet" href="/css/css.css" type="text/css">
</head>

<script language="javascript">


function goBack()
{
	history.back();
	return false;
}


</script>

<body bgcolor="#DEE8FA">
<table width="100%" border="0" align="center" cellspacing="0" cellpadding="0" bgcolor="#DEE8FA">
  
  <tr> 
    <td align="center"> 
    	<form action="/pronline/Msg" method="post"  name="form1" >
    	<input name="FunName" type="hidden" value="webProcy_imp">
    	<input type="hidden" name="gtype"   value="[#gtype#]">
      <input type="hidden" name="id"   value="[#id#]">
      <input type="hidden" name="flags" value="[#flags#]">
        <table width="100%" border="0" align="center" cellspacing="0" cellpadding="0">
          <tr> 
            <td align="center"> 
              <table width="100%" border="1" align="center" bordercolorlight=#89ACED   bordercolordark=#ffffff cellspacing="0" cellpadding="0">

                <tr> 
                  <td align="center">
                    <table width="100%" border="0" align="center" cellpadding="0"  bgcolor="#DEE8FA">
                     <tr>
                        <td height="25"><div align="right">序号&nbsp;</div></td>
                        <td height="25" valign="baseline">
                        	<input type="text" name="rnum" value="0001" maxlength="4" style="width:100px">
                        	</td>
                      </tr>
                      <tr>
                        <td width="16%" height="25"><div align="right">时间段从&nbsp;</div></td>
                        <td width="84%" height="25" valign="baseline">
                       [#dateid#] 
                      <input type="text" name="stime" size="8" value="00:00:00" maxlength="8">
                      到 
                      <input type="text" name="etime" size="8" value="23:59:59" maxlength="8">
                        </td>
                      </tr>
                      <tr>
                        <td height="25"><div align="right">控制方式&nbsp;</div></td>
                        <td height="25" valign="baseline">
                      <select name="limctl" >
                  <option value="0" selected>禁止</option>
                  <option value="1">允许</option>
                  <option value="3" >告警</option>
                  <option value="4" >免监控</option>
                      [#SLPB#]
                      <option value="[#qosid#]">[#qosname#]</option>
                       [#SLPE#]
                      </select>
                        </td>
                      </tr>
                      <tr>
                        <td height="25"><div align="right">网址包含&nbsp;</div></td>
                        <td height="25" valign="baseline">
                        <select name="urlname" style="width:300px">
                       <option value="[#url_num#]">[#urlname#]</option>
            			      [#SLPB#]
                        <option value="[#iNum#]">[#caTemp#]</option>
                        [#SLPE#]
                       
                        </select>
                        </td>
                      </tr>
                    
                   
                    
                      <tr>
                        <td height="25"><div align="right">忽略第一行</div></td>
                        <td height="25" valign="baseline"><input type="checkbox" name="first" value="checkbo"  >

                        	</td>
                      </tr>
                      <tr>
                        <td height="25"><div align="right"> </div></td>
                        <td height="25" valign="baseline" id="xxm2"> <input name="ok" type="button" class="anuse3" id="ok" value="确 认" onclick="form1.submit();document.all.ok.disabled=1;document.all.xxm2.innerHTML='<font color=red size=3>正在导入,请稍候...</font>' ">
                        <input name="return" type="button" onClick="javascript:history.back();"   class="anuse3" value="返 回"></td>
                      </tr>
                     
                    </table></td>
                </tr>
              </table>
</td>

          </tr>
        </table>
      </form></td>
  </tr>
</table>
<br>
</body>
</html>
