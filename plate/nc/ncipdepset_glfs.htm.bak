function showf(s1,s2,s3,s4,netf) {
    form1.sip.value=s1;
    form1.eip.value=s2;
    form1.sid.value=s3;
    form1.groupid.value=s4;
    form1.flags1.value=netf;
}
    function Add()
    {
	    if(form1.sip.value==''||form1.eip.value=='')
	    {
	    	alert('起始ip和终止IP不能为空');
	    	return false;
	    	}
	    	return true;
    }
    function Mod()
    {
	    if(form1.sid.value=='')
	    {
	    	alert('请先选择要修改的内容');
	    	return false;
	    	}

	    	return true;
    }
    function Del()
    {
    var flag=0;
    	    for(var i=1;i<document.all.DataTable.rows.length;i++)
	    {
	    if(document.all.DataTable.rows[i].cells[0].childNodes[0].checked)
	    {
		    flag=1;
		    break;
	    }
	    }
	    if(flag==0)
	    {
	    	alert('请先选择要删除的内容');
	    	return false;
	    }
	    	return true;
    }
    
function mouseouttr2(o){
