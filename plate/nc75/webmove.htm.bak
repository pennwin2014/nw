<body topmargin=5 leftmargin=0 bgcolor="#DEE8FA" scroll=AUTO >
	<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
    <form name="form1" method="post" action="/pronline/Msg">
    <input type="hidden" name="save" value="save">
  <input type="hidden" name="FunName" value="ncUserForbit">

  <tr> 
    <td colspan="2"><table width="100%" height="35" border="0" align="center" cellpadding="0" cellspacing="0" >
        <tr > 
          <td colspan="2" > <div style="font-size:11pt;color:#00237e;font-weight:bold;text-align: center;">
             
              移动网址[#mesg#]</div></td>
        </tr>
      </table></td>
  </tr>

<tr> 
    <td colspan="2"> 
        
        <table width="99%" border="1"    bordercolorlight=#89ACED   bordercolordark=#ffffff    align="center" cellpadding="0" cellspacing="0" >
          <tr height="30"> 
            <td colspan="2">&nbsp;
            	网址名称&nbsp;<input type="text" name="weburl" size="20" value="[#weburl#]" >

            	类别&nbsp;<select name="cid">[#SLPB#]<option value="[#id#]">[#name#]</option>[#SLPE#]</select>

            	&nbsp;&nbsp;<input name="submit1" type="button" class="an3"   onclick="OK()" value="确认" >
            
            	</td>
          </tr>
           <tr height="30"> 
            <td colspan="2">&nbsp;
            	网址说明&nbsp;<input type="text" name="webdesc" size="38" value="[#urldesc#]" >

            	</td>
          </tr>
       
        </table>
     </td>
    </tr>
	</table>
	
</form>
<script>if('[#id#]'!='') form1.cid.value='[#id#]';</script>
