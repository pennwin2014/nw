<script type="text/javascript">
	
   cp = [#CurPg#];
   tp = [#TotPg#];
   function gotoPage(t){
       if(t==1) {
           form1.app.value = "next";
       }
       else if(t == (-1)) {
           form1.app.value = "prev";
       }
       cp = cp + t;
       if(cp <= 0) cp = 1;
       if(cp > tp) cp = tp;
       
       form1.CurPg.value = cp;
       form1.submit();
       return;
   }
	
	
	
