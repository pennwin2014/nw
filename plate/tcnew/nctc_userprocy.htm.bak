  document.all.uid1.style.display='none';
  }
}

function qr(){
if(form1.utype.value=='2'){

	var temp="/pronline/Msg?FunName@ncLimMonUser&rycx@"+form1.udispname.value+"&userid@"+form1.userid.value;
	
	showModalDialog(temp, window, 'status:no;center:yes;help:no;minimize:no;maximize:yew;border:thick;statusbar:no;dialogWidth:520px;scroll:yes;dialogHeight:470px');
	return false;
}
return true;
}


